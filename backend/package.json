{
  "name": "backend",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --config webpack.config.js --mode=production",
    "start": "node ./dist/index.js",
    "dev": "webpack --watch",
    "format:write": "prettier . --write",
    "format:check": "prettier . --check",
    "lint:check": "eslint",
    "prepare": "cd .. && husky ./.husky",
    "lint-staged": "lint-staged",
    "test:unit": "jest unit --detectOpenHandles",
    "test:int": "jest int --detectOpenHandles",
    "seed": "webpack --entry-reset --entry ./tasks/seed.js --output-path ./dist --output-filename seed-bundle.js --mode=development && node ./dist/seed-bundle.js"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "description": "",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^9.23.0",
    "@shelf/jest-mongodb": "^5.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jasmine": "^5.6.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "nodemon-webpack-plugin": "^4.8.2",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "supertest-session": "^5.0.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "mongodb": "^6.15.0",
    "validator": "^13.15.0",
    "zod": "^3.24.2"
  },
  "lint-staged": {
    "**/*.js": [
      "npm run lint:check",
      "npm run format:check",
      "npm run test:unit",
      "npm run test:int"
    ]
  }
}
